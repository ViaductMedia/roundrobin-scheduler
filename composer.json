{
    "name": "viaductmedia/roundrobin-scheduler",
    "description": "Utils for round-robin tournaments. Laravel ready.",
    "keywords": [
        "library",
        "laravel",
        "round-robin",
        "tournaments",
        "berger",
        "teams",
        "scheduler"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Luca Gentile",
            "email": "info@lucagentile.eu"
        }
    ],
    "require": {
        "php": ">=7"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.2",
        "laravel/framework": "^7.22",
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Gautile\\RoundRobin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gautile\\RoundRobin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gautile\\RoundRobin\\RoundRobinSchedulerServiceProvider"
            ]
        }
    }
}
